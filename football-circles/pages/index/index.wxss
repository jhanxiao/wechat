
page{
  height:100%;
  display:flex;
  flex-direction:column;
}
.header{
  height:103.125rpx;
}
.tab{
  width:100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  background: #0DC441;
  height:100%;
  align-items: center; 
}
.tab view{
  width:141.796875rpx;
  height:51.5625rpx;
  background: #3DD067;
  line-height: 51.5625rpx;
  text-align: center;
  color:#A3E9B7;
  font-size:28.125rpx;
}
.tab view:first-child{
  border-top-left-radius:25.78125rpx ;
  border-bottom-left-radius: 25.78125rpx;  
}
.tab view:last-child{
  border-top-right-radius:25.78125rpx ;
  border-bottom-right-radius: 25.78125rpx;  
}
.tab .navActive{
  background: #64D985;
  color:#fff;
}

.content{
  flex:1;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.scrollbox{
  flex:1;
}
swiper{
  height:100%;
}
.swiper-nav{
  box-sizing: border-box;
  width:100%;
  height:82.03125rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom:1px solid #d9d9d9;
  background:rgba(255,255,255,0.5);
}
swiper swiper-item{
  overflow-y: scroll;
}
.swiper-nav>view{
  box-sizing: border-box;
  width:234.375rpx;
  height:100%;
  text-align: center;
  line-height: 82.03125rpx;
  color:#666666;
  border-bottom:5.859375rpx solid transparent;
}
.swiper-nav>.nav-active{
  color:#3DD067;
  border-bottom:5.859375rpx solid #3DD067;
}
.live,.life,.girls{
  width:100%;
  display:flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
}
.live>view{
  width:369.140625rpx;
  border:1px solid #e5e5e5;
  box-sizing: border-box;
  margin-top:9.375rpx;
}
.life>view{
  width:369.140625rpx;
  border:1px solid #e5e5e5;
  box-sizing: border-box;
  margin-top:9.375rpx;
}
.girls>view{
  width:369.140625rpx;
  border:1px solid #e5e5e5;
  box-sizing: border-box;
  margin-top:9.375rpx;
}
swiper image{
  width:100%;
  height:369.140625rpx;
  border-bottom:1px solid #e5e5e5;
}
.live .title{
  box-sizing: border-box;
  height:67.96875rpx;
  line-height: 67.96875rpx;
  padding-left:23.4375rpx;
  color:#333333;
}
.footer{
  width:100%;
  height:103.125rpx;
  display: flex;
  flex-direction: row;
  border-top:1px solid #cbd7cc;
}
.footer>view{
  width:20%;
  height:100%;
  text-align: center;
  font-size: 30rpx;
  color:#d0d1ce;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.footer .page-active{
  color:#0dc441;
}
.footer .camera{
  box-sizing: border-box;
  font-size: 50rpx;
  line-height:124.21875rpx;
  width:124.21875rpx;
  height: 124.21875rpx;
  margin:-21.09375rpx auto 0 auto;
  border:1px solid #cbd7cc;
  border-radius: 50%;
  position: relative;
  z-index:9;
  background:#fff;
}
